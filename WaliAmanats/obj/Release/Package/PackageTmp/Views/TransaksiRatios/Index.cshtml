				"dom": "<'row'<'col-sm-4'B><'col-sm-2'l><'col-sm-6'p>>" + "<'row'<'col-sm-12'tr>>" + "<'row'<'col-sm-5'i><'col-sm-7'p>>",
				"columnDefs": [
					{

						"visible": false,
						"searchable": false
					},
					{
						"searchPanes": {
							"show": true,
